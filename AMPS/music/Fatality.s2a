	saVol		$FE
	dc.b $02
	saVol		$FE
	dc.b $01, $02
	saVol		$FE
	dc.b $01, $01
	saVol		$FD
	dc.b sHold, $01, $01
	saVol		$FB
	dc.b $02, $01
	sStop

; FM5 Data
Snd_FatalityUMK3_FM5:
