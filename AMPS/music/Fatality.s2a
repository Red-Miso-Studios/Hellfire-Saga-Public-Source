	dc.b nC5, $02, nRst, $01, nFs5, $02, nRst, $01, nC7, $02, $01, $02
	dc.b $01

Snd_FatalityUMK3_Loop00:
	dc.b $02
	saVol		$FF
	dc.b $01
	saVol		$FF
	dc.b $02
	saVol		$FF
	dc.b $01
	saVol		$FE
	sLoop		$00, $03, Snd_FatalityUMK3_Loop00
	dc.b $02
	saVol		$FF
	dc.b $01
	saVol		$FF
	dc.b $02
	saVol		$FF
	dc.b $01
	sStop

